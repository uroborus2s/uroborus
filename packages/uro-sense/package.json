{
  "name": "@uroborus/sense-ui",
  "version": "0.0.1-alpha.7",
  "description": "可无限扩展的Reaction UI组件库。",
  "author": "snails336",
  "homepage": "",
  "main": "build/node/index.js",
  "module": "build/index.js",
  "types": "build/index.d.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "build": "pnpm prebuild && pnpm build:legacy && pnpm build:stable && pnpm build:modern && pnpm build:node && pnpm build:types",
    "build:legacy": "BROWSERSLIST_ENV=legacy esno ../../scripts/build.ts legacy",
    "build:modern": "BROWSERSLIST_ENV=modern esno ../../scripts/build.ts modern",
    "build:node": "BROWSERSLIST_ENV=node esno ../../scripts/build.ts node",
    "build:stable": "BROWSERSLIST_ENV=stable esno ../../scripts/build.ts stable",
    "build:types": "tsc -p tsconfig.build.json",
    "prebuild": "rimraf build tsconfig.build.tsbuildinfo"
  },
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/node/index.js",
      "types": "./build/index.d.ts"
    },
    "./CircularProgress": {
      "import": "./build/CircularProgress/index.js",
      "require": "./build/node/CircularProgress/index.js",
      "default": "./build/CircularProgress/index.js",
      "types": "./src/CircularProgress/index.d.ts"
    },
    "./CssVarsProvider": {
      "import": "./build/CssVarsProvider/index.js",
      "default": "./build/CssVarsProvider/index.js",
      "types": "./src/CssVarsProvider/index.d.ts"
    },
    "./styles": {
      "import": "./build/styles/index.js",
      "default": "./build/node/styles/index.js",
      "types": "./src/styles/index.d.ts"
    },
    "./util": {
      "import": "./build/util.js",
      "default": "./build/node/util.js",
      "types": "./src/build/util.d.js"
    }
  },
  "peerDependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "^11.10.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@uroborus/core": "workspace:*",
    "@uroborus/hooks": "workspace:*",
    "classnames": "^2.3.2",
    "@mui/system": "^5.11.5",
    "@mui/base": "5.0.0-alpha.120"
  },
  "engines": {
    "node": ">=16.18.1"
  }
}
